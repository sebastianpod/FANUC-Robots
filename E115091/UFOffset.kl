PROGRAM UFOffset
%NOPAUSESHFT
%NOLOCKGROUP
%NOBUSYLAMP
%NOABORT= ERROR + TPENABLE
%NOPAUSE= ERROR + TPENABLE

VAR
entry:INTEGER
sts:INTEGER
orguf:POSITION
cor:POSITION
ufcor:POSITION

BEGIN
	cor=GET_POS_REG(4,sts)
	orguf=GET_POS_REG(20,sts)
	ufcor=cor:orguf
	SET_VAR(entry,'*SYSTEM*','$MNUFRAME[1,1]',ufcor,sts)
END UFOffset
	