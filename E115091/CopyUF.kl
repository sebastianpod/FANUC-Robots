PROGRAM CopyUF
%NOPAUSESHFT
%NOLOCKGROUP
%NOBUSYLAMP
%NOABORT= ERROR + TPENABLE
%NOPAUSE= ERROR + TPENABLE

VAR
entry: INTEGER
sts: INTEGER
uf:POSITION

BEGIN
	GET_VAR(entry,'*SYSTEM*','$MNUFRAME[1,4]',uf,sts)
	SET_VAR(entry,'*SYSTEM*','$MNUFRAME[1,1]',uf,sts)
	SET_POS_REG(20,uf,sts)
	
END CopyUF
